{
  "name": "marttosc/php-dbf-sql",
  "description": "A simple converter from DBF to SQL",
  "keywords": [
    "marttosc", "dbf", "sql", "dbase", "parser", "php", "mysql"
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Gustavo Marttos",
      "email": "marttosc@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "hisamu/php-xbase": "^1.0.1",
    "illuminate/support": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Marttos\\DbfSql\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Marttos\\DbfSql\\Testing\\": "tests/"
    }
  }
}
