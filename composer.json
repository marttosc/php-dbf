    "marttosc", "dbf", "reader", "dbase", "parser", "php"
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Gustavo Marttos",
      "email": "marttosc@gmail.com"
    }
  ],
  "require": {
